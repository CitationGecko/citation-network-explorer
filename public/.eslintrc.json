{
  "extends": "airbnb-base",
  "env": {
    "node": false,
    "browser": true,
    "es6": false
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-var": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "prefer-numeric-literals": 0,
    "prefer-destructuring": 0,
    "consistent-return": 1,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-empty": 0,
    "object-shorthand": 0,
    "no-mixed-operators": 0,
    "use-isnan": 0,
    "eqeqeq": 1,
    "no-useless-concat": 1,
    "object-curly-newline": 1,
    "indent": 0,
    "no-trailing-spaces": 0,
    "object-curly-spacing": 0,
    "space-infix-ops": 0,
    "block-spacing": 0,
    "comma-spacing": 0,
    "no-shadow": 0,
    "quotes": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 1,
    "semi": 1
  }
}
